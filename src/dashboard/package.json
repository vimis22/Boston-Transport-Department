{
  "name": "dashboard",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build --compile --target bun --outfile server src/index.ts",
    "tailwind:build": "tailwindcss -i ./public/styles/global.css -o ./public/styles/global.dist.css",
    "tailwind:watch": "tailwindcss -i ./public/styles/global.css -o ./public/styles/global.dist.css --watch"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/static": "^1.4.7",
    "@tanstack/react-query": "^5.90.12",
    "clsx": "^2.1.1",
    "elysia": "^1.4.18",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.5.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@types/bun": "^1.3.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "bun-types": "^1.3.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
